{
  "name": "alec-react-router",
  "version": "0.0.5",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/alec016/alec-react-router",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "prepare": "npx swc ./src/Link.tsx ./src/Route.tsx ./src/Router.tsx ./src/index.tsx ./src/utils/ -d lib"
  },
  "dependencies": {
    "path-to-regexp": "6.2.1"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.59",
    "@testing-library/dom": "9.3.0",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vitejs/plugin-react-swc": "3.0.0",
    "@vitest/ui": "^0.31.1",
    "eslint": "8.38.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "happy-dom": "9.18.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "standard": "17.0.0",
    "typescript": "5.0.2",
    "vite": "4.3.2",
    "vitest": "0.31.1"
  }
}